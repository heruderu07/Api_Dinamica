generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Plan {
  BASIC
  STANDARD
  PREMIUM
}

enum Gender {
  MALE
  FEMALE
  OTHER
}

model Customer {
  id         Int      @id @default(autoincrement()) 
  name       String
  email      String   @unique
  gender     Gender  
  country    String  
  plan       Plan
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}

model USD_BRL {
  id          Int      @id @default(autoincrement())
  create_date DateTime @unique
  high        Float
  low         Float
  volume      Float
  createdAt   DateTime @default(now())
}